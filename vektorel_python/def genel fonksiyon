def topla(aa,bb):
    return f"Toplam:{aa+bb}"

def carp(x,yy):
    return f"Çarpım:{x*yy}"

#sayi1 = 3
#sayi2 = 7
sayi1 = int(input("Birinci sayı nedir?"))
sayi2 = int(input("İkinci sayı nedir?"))

#topla(sayi1,sayi2) tek başına işlemi yapar ancak bir yerde görünmez
print(topla(sayi1,sayi2))

#fonksiyonlarda geri dönüş olmadan bu şekilde kullanılmaz
#topla(sayi1,topla(sayi2,sayi2))

print(carp(sayi1,sayi2))